require([
    'jquery',
    'swiper',
    'underscore'
], function($, Swiper, _){
    "use strict";

    var elem = ".swiper-container.stories";

    var extraOptions = {
        centerContent: function(swiper, value){
            $(swiper.slides).css({
                display: "flex",
                flexDirection: "column",
                alignItems: "center"
            })
        }
    }

    var extraParams = {
        on: {
            init: function (swiper) {
                var params = swiper.params;
                var sliderElements = [];

                sliderElements.push(params.pagination.el);
                sliderElements.push(params.navigation.nextEl);
                sliderElements.push(params.navigation.prevEl);
                sliderElements.push(params.scrollbar.el);

                sliderElements.forEach(function(elClass){
                    if(elClass) {
                        elClass = elClass.replace(/^./g, '');
                        $(elem).append("<div class='" + elClass + "'></div>");
                    }
                });

                if(params.extra) {
                    var extras = _.pairs(params.extra);
                    extras.forEach(function(value){
                        extraOptions[value[0]](swiper, value[1])
                    })
                }
            }
        }
    }

    var conf = _.extend({
        "slidesPerView": 3,
            "freeMode": true,
            "freeModeMomentumRatio": 0.7,
            "freeModeMomentumVelocityRatio": 0.8,
            "spaceBetween": 15,
            "watchOverflow": true,
            "centerInsufficientSlides": true,
            "roundLengths": true,
            "slidesOffsetBefore": 20,
            "slidesOffsetAfter": 20,
            "extra": {
                "centerContent": true
            },
            "breakpoints": {
                "1": {
                    "slidesPerView": 4.55
                },
                "550": {
                    "slidesPerView": 5.55
                },
                "800": {
                    "slidesPerView": 6.55
                },
                "1024": {
                    "slidesPerView": 7,
                    "navigation": {
                        "nextEl": ".swiper-button-next",
                        "prevEl": ".swiper-button-prev"
                    }
                },
                "1410": {
                    "slidesPerView": 7,
                    "navigation": {
                        "nextEl": ".swiper-button-next",
                        "prevEl": ".swiper-button-prev"
                    },
                    "slidesOffsetBefore": 0,
                    "slidesOffsetAfter": 0
                }
            }
    }, extraParams);

    var mySwiper = new Swiper(elem, conf);
});
</script>
